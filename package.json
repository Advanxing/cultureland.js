{
    "name": "cultureland.js",
    "version": "3.0.0-dev",
    "description": "컬쳐랜드 모바일웹을 자동화해주는 비공식 라이브러리입니다.",
    "type": "commonjs",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.mts",
    "scripts": {
        "build": "tsup"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Gitland/Cultureland-API.git"
    },
    "keywords": [
        "cultureland",
        "transkey",
        "mtranskey",
        "raonsecure",
        "charge"
    ],
    "license": "GPL-3.0",
    "bugs": {
        "url": "https://github.com/Gitland/Cultureland-API/issues"
    },
    "homepage": "https://github.com/Gitland/Cultureland-API#readme",
    "dependencies": {
        "@modern-dev/jsbn": "^1.2.5",
        "axios": "^1.4.0",
        "http-cookie-agent": "^5.0.4",
        "jimp": "^0.16.2",
        "node-html-parser": "^6.1.12",
        "node-rsa": "^1.1.1",
        "tough-cookie": "^4.1.3"
    },
    "devDependencies": {
        "@types/node-rsa": "^1.1.4",
        "@types/tough-cookie": "^4.0.5",
        "tsup": "^8.1.0",
        "typescript": "^5.5.3"
    }
}
